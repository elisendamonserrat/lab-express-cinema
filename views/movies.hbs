<div class="container">
     <h1 class="h1--black">Ironhack Cinema</h1>
     <p>Click on the movie to see the showtime!</p>
     <div class="movies">
    {{#each moviesArray}}
        <div class="movie">
            <img src="{{image}}" alt="">
            <h3>{{title}}</h2>
            <a href="/movies/{{this._id}}">See more</a>
        </div>
    {{/each}}
    </div>
</div>